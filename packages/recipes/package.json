{
  "name": "@hama-ui/recipes",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.mjs"
    },
    "./components/*": {
      "types": "./dist/components/*.d.ts",
      "default": "./dist/components/*.mjs"
    },
    "./components/*.js": {
      "types": "./dist/components/*.d.ts",
      "default": "./dist/components/*.mjs"
    },
    "./components/**/*": {
      "types": "./dist/components/**/*.d.ts",
      "default": "./dist/components/**/*.mjs"
    },
    "./components/**/*.js": {
      "types": "./dist/components/**/*.d.ts",
      "default": "./dist/components/**/*.mjs"
    }
  },
  "scripts": {
    "build": "unbuild",
    "stub": "unbuild --stub"
  },
  "dependencies": {
    "cva": "1.0.0-beta.1"
  }
}
